{
  "name": "ae-agent-portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/react": "^5.19.0",
    "date-fns": "^2.16.1",
    "focus-visible": "^5.1.0",
    "formik": "^2.1.5",
    "oidc-client": "^1.10.1",
    "react": "^16.13.1",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.1",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.14",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^7.1.2",
    "chromedriver": "^83.0.1",
    "cross-env": "^7.0.2",
    "geckodriver": "^1.19.1",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "nightwatch": "^1.3.6",
    "node-sass": "^4.14.1",
    "selenium-server": "^3.141.59",
    "sharp": "^0.26.0"
  },
  "scripts": {
    "start": "rescripts start",
    "start:auth": "cross-env REACT_APP_BUILD_TARGET=auth PORT=3001 BROWSER=none rescripts start",
    "build": "yarn test && rescripts build",
    "build:auth": "cross-env REACT_APP_BUILD_TARGET=auth rescripts build && mv build buildAuth",
    "test": "rescripts test --env=jest-environment-jsdom-sixteen",
    "e2e:local": "nightwatch -c tests/conf/nightwatch-local.conf.js",
    "e2e:browserstack": "nightwatch -c tests/conf/browserstack.conf.js -e chrome,firefox",
    "eject": "rescripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {}
}
